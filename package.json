{
	"name": "phoenix-config",
	"version": "1.0.0",
	"description": "Configuration for phoenix written in TypeScript",
	"main": "src/phoenix.ts",
	"dependencies": {
		"typescript": "^3.0.1",
		"typings": "^2.1.1"
	},
	"devDependencies": {
		"prettier": "^1.14.2",
		"ts-loader": "^4.4.2",
		"tslint": "^5.11.0",
		"webpack": "^4.16.5",
		"webpack-cli": "^3.1.0"
	},
	"scripts": {
		"start": "npm run watch",
		"watch": "webpack --watch",
		"build": "webpack",
		"typings": "typings install",
		"lint": "tslint src/**/*.ts",
		"format": "prettier --write src/**/*.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/elliot-evans-95/phoenix-config.git"
	},
	"keywords": [
		"phoenix",
		"typescript"
	],
	"author": "Elliot Evans",
	"license": "MIT",
	"prettier": {
		"tabWidth": 4,
		"useTabs": true,
		"singleQuote": true,
		"bracketSpacing": true,
		"trailingComma": "all"
	}
}
